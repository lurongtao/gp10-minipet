.container {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
}

.container .map {
  flex: 1;
}

.container .map map {
  width: 100%;
  height: 100%;
}

.container .tabbar {
  height: 88rpx;
  display: flex;
}

.container .tabbar view {
  flex: 1;
  text-align: center;
  color: #fff;
  line-height: 88rpx;
}

.container .tabbar view:first-child {
  background: #f29a38;
}

.container .tabbar view:last-child {
  background: #52b9d2;
}